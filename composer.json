{
    "name": "php-task-runner/sparql-robo-tasks",
    "description": "Provides SPARQL commands for Robo",
    "keywords": ["task-runner"],
    "type": "robo-tasks",
    "require": {
        "php": ">=7.4",
        "consolidation/robo": "^1.0 || ^2.0 || ^3.0 || ^4",
        "easyrdf/easyrdf": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7 || ~8 || ~9",
        "slevomat/coding-standard": "~6",
        "squizlabs/php_codesniffer": "~3"
    },
    "license": "EUPL-1.2",
    "authors": [
        {
            "name": "Claudiu Cristea",
            "email": "clau.cristea@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Robo\\Sparql\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Robo\\Sparql\\Tests\\": "./tests/src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
